
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<html>
<table class="table table-condensed">
    <thead>
        <tr>
            <th>Product ID</th>
            <th>Product Name</th>
            <th>Product Description</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
         {
                <tr>
                    <td>
                        @p.Id
                    </td>
                    <td>
                        @p.Name
                       
                    </td>
                    <td>
                        @p.Description
                    </td>

                </tr>
         }
    </tbody>
</table>
</html>
